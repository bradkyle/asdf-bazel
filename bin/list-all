#!/usr/bin/env bash

# redsymbol.net/articles/unofficial-bash-strict-mode/
set -o nounset -o pipefail -o errexit
IFS=$'\n\t'

versions_list=(
  0.1.1
  0.1.2
  0.1.3
  0.1.4
  0.1.5
  0.2.0
  0.2.1
  0.2.2
  0.2.2b
  0.2.3
  0.3.0
  0.3.1
  0.3.2
  0.4.0
  0.4.1
  0.4.2
  0.4.3
  0.4.4
  0.4.5
  0.5.0
  0.5.1
  0.5.2
  0.5.3
  0.5.4
  0.6.0
  0.6.1
  0.7.0
  0.8.0
  0.8.1
  0.9.0
  0.10.0
  0.10.1
  0.11.0
  0.11.1
  0.12.0
  0.13.0
)

versions=""

# Concatenate all the versions together separated by spaces
for version in "${versions_list[@]}"
do
  versions="${versions} ${version}"
done

# Print out the versions
echo $versions | xargs
